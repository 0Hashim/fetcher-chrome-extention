{
    "name": "fetch",
    "description": "fast right click search",
    "version": "0.1",
    "manifest_version": 3,

    "action":{
    "default_icon":{"128":"icons/rover.png"},
    "default_popup":"popup/popup.html"
    },

    "icons":{
        "16":"icons/rover.png",
        "128":"icons/rover.png"
    },

    "background":{
        "service_worker":"background.js"
    },
    
    "permissions": ["contextMenus","tabs","storage"] 
  }